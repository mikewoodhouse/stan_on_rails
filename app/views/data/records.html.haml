#data

:javascript
    function tabulate(report) {
        let col_names = report.display_cols
        let table = document.createElement('table')
        let tr = table.insertRow(-1)
        for (let i = 0; i < col_names.length; i++) {
            let th = document.createElement('th')
            th.innerHTML = col_names[i]
            tr.appendChild(th)
        }
        for(let i = 0; i < report.data.length; i++) {
            tr = table.insertRow(-1)
            for (let j = 0; j < col_names.length; j++) {
                let cel = tr.insertCell(-1)
                cel.innerHTML = report.data[i][col_names[j]]
            }
        }
        return table
    }

    document.addEventListener('DOMContentLoaded', (event) => {
        let elem = document.getElementById('data')
        elem.innerHtml = ""

        let xhttp = new XMLHttpRequest()

        xhttp.onload = function() {
            if (this.readyState == 4 && this.status == 200) {
                data = JSON.parse(this.responseText);
                elem.appendChild(tabulate(data));
            }
        }

        xhttp.open("GET", "data/get/players", true)
        xhttp.send()

    })